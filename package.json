{
  "name": "bem-vue",
  "version": "0.1.8",
  "description": "Core package helps organize and manage components with BEM modifiers in Vue.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "compile": "tsc",
    "test": "jest",
    "build": "npm run test && npm run compile",
    "publish": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sp1ker/bem-vue.git"
  },
  "keywords": [
    "bem",
    "vue",
    "vuejs"
  ],
  "author": "Gavrilov Evgeniy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sp1ker/bem-vue/issues"
  },
  "homepage": "https://github.com/sp1ker/bem-vue#readme",
  "dependencies": {
    "@bem-react/classname": "1.5.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@types/jasmine": "^3.3.13",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-jest": "^24.8.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^24.8.0",
    "regenerator-runtime": "^0.13.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  }
}
